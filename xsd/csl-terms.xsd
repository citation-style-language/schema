          <xs:enumeration value="season-03"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="season-04"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="cs:category.field"/>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>
  <xs:simpleType name="terms.gender-assignable">
    <xs:annotation>
      <xs:documentation>Terms to which a gender may be assigned</xs:documentation>
    </xs:annotation>
    <xs:union memberTypes="cs:terms.non-locator-number-variables cs:terms.locator-number-variables">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="month-01">
            <xs:annotation>
              <xs:documentation>Months</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="month-02"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="month-03"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="month-04"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="month-05"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="month-06"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="month-07"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="month-08"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="month-09"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="month-10"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="month-11"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="month-12"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>
  <xs:simpleType name="terms.gender-variants">
    <xs:annotation>
      <xs:documentation>Terms for which gender variants may be specified</xs:documentation>
    </xs:annotation>
    <xs:union memberTypes="cs:terms.ordinals cs:terms.long-ordinals"/>
  </xs:simpleType>
  <xs:simpleType name="terms.ordinals">
    <xs:restriction base="xs:string">
      <xs:pattern value="ordinal(-\d{2})?"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="terms.long-ordinals">
    <xs:restriction base="xs:token">
      <xs:enumeration value="long-ordinal-01">
        <xs:annotation>
          <xs:documentation>Long ordinals</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="long-ordinal-02"/>
      <xs:enumeration value="long-ordinal-03"/>
      <xs:enumeration value="long-ordinal-04"/>
      <xs:enumeration value="long-ordinal-05"/>
      <xs:enumeration value="long-ordinal-06"/>
      <xs:enumeration value="long-ordinal-07"/>
      <xs:enumeration value="long-ordinal-08"/>
      <xs:enumeration value="long-ordinal-09"/>
      <xs:enumeration value="long-ordinal-10"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="terms.locator">
    <xs:annotation>
      <xs:documentation>Locators</xs:documentation>
    </xs:annotation>
    <xs:union memberTypes="cs:terms.locator.testable">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="sub verbo">
            <xs:annotation>
              <xs:documentation>"sub verbo" is recognized as "sub" &amp; "verbo" in attribute lists; term
should be renamed to "sub-verbo"</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>
  <xs:simpleType name="terms.locator.testable">
    <xs:annotation>
      <xs:documentation>Locator terms that can be tested with the "locator" conditional
("sub verbo" can be tested with "sub-verbo")</xs:documentation>
    </xs:annotation>
    <xs:union memberTypes="cs:terms.locator-number-variables">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="book"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="chapter"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="column"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="figure"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="folio"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="line"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="note"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="opus"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="page"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="paragraph"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="part"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="section"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="verse"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>
  <xs:simpleType name="terms.locator-number-variables">
    <xs:annotation>
      <xs:documentation>Locator terms with matching number variables</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="issue"/>
      <xs:enumeration value="volume"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="terms.non-locator-number-variables">
    <xs:annotation>
      <xs:documentation>Non-locator terms accompanying number variables</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="chapter-number"/>
      <xs:enumeration value="collection-number"/>
      <xs:enumeration value="edition"/>
      <xs:enumeration value="number"/>
      <xs:enumeration value="number-of-pages"/>
      <xs:enumeration value="number-of-volumes"/>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>
